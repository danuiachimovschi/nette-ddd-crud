{
	"name": "nette/web-project",
	"description": "Nette: Standard Web Project",
	"keywords": ["nette"],
	"type": "project",
	"license": ["MIT", "BSD-3-Clause", "GPL-2.0-only", "GPL-3.0-only"],
	"require": {
		"php": ">= 8.1",
		"nette/application": "^3.2.3",
		"nette/bootstrap": "^3.2",
		"nette/caching": "^3.2",
		"nette/database": "^3.2",
		"nette/di": "^3.2",
		"nette/forms": "^3.2",
		"nette/http": "^3.3",
		"nette/mail": "^4.0",
		"nette/robot-loader": "^4.0",
		"nette/security": "^3.2",
		"nette/utils": "^4.0",
		"latte/latte": "^3.0",
		"tracy/tracy": "^2.10",
		"nettrine/migrations": "^0.9.1",
		"nettrine/dbal": "^0.8.2",
		"nettrine/orm": "^0.8.4",
		"contributte/console": "^0.10.1",
		"contributte/middlewares": "^0.12.1",
		"contributte/messenger": "^0.1.0",
		"contributte/event-dispatcher": "^0.9.1"
	},
	"require-dev": {
		"nette/tester": "^2.5",
		"symfony/thanks": "^1",
		"phpstan/phpstan": "^1.12"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app",
			"YourProjectName\\": "src"
		}
	},
	"minimum-stability": "stable",
	"config": {
		"allow-plugins": {
			"symfony/thanks": true
		}
	},
	"scripts": {
		"phpstan": [
			"vendor/bin/phpstan analyse src"
		]
	}
}
